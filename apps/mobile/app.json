{
    "expo": {
        "name": "LifeSCC",
        "slug": "lifescc-mobile",
        "version": "1.0.0",
        "orientation": "portrait",
        "userInterfaceStyle": "light",
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.lifescc.mobile"
        },
        "android": {
            "package": "com.lifescc.mobile"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "expo-router"
        ],
        "scheme": "lifescc"
    }
}